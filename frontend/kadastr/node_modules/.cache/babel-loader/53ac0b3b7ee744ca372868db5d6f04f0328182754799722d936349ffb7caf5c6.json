en\");\n      const headers = {\n        \"Content-Type\": \"application/json\",\n        Authorization: \"Bearer \" + token,\n      };\n      const url = this.$BASE_URL + \"/api/add_district/\";\n      axios\n        .post(\n          url,\n          { area_id: this.areaId, district_name: this.districtName },\n          { headers }\n        )\n        .then((response) => {\n          if (response.data[0].auth_fail) {\n            this.showAuthError = true;\n            return;\n          }\n\n          this.districtName = \"\";\n        });\n      e.preventDefault();\n    },\n  },\n  components: {\n    BootstrapIcon,\n    OkModal,\n  },\n  mounted() {},\n};\n</script>\n\n<style scoped>\n.item {\n  width: calc(100%);\n  height: 50px;\n  font-weight: bolder;\n  font-family: \"Gill Sans\", \"Gill Sans MT\", Calibri, \"Trebuchet MS\", sans-serif;\n  border-style: none !important;\n}\n</style>\n"],"mappings":"AAkCA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,aAAY,MAAO,iCAAiC;AAC3D,OAAOC,OAAM,MAAO,eAAe;AAEnC,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAK,EAAE,CAAC,aAAa,EAAE,SAAS,CAAC;EACjCC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,IAAI,CAACC,OAAO;MACpBC,UAAU,EAAE,IAAI,CAACC,WAAW;MAC5BC,YAAY,EAAE,EAAE;MAChBC,aAAa,EAAE,KAAK;MACpBC,WAAW,EAAE,iBAAiB;MAC9BC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,KAAK,EAAE;IACLP,OAAO,EAAE;MACPQ,IAAI,EAAE,IAAI;MACVC,OAAOA,CAACC,CAAC,EAAE;QACT,IAAI,CAACX,MAAK,GAAIW,CAAC;MACjB;IACF,CAAC;IACDR,WAAW,EAAE;MACXM,IAAI,EAAE,IAAI;MACVC,OAAOA,CAACC,CAAC,EAAE;QACT,IAAI,CAACT,UAAS,GAAIS,CAAC;MACrB;IACF;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACC,KAAK,CAAC,kBAAkB,EAAE,CAAC,CAAC,CAAC;MAClC,IAAI,CAACT,aAAY,GAAI,KAAK;IAC5B,CAAC;IACDU,OAAOA,CAACC,CAAC,EAAE;MACT,MAAMC,KAAI,GAAIC,cAAc,CAACC,OAAO,CAAC,OAAO,CAAC;MAC7C,MAAMC,OAAM,GAAI;QACd,cAAc,EAAE,kBAAkB;QAClCC,aAAa,EAAE,SAAQ,GAAIJ;MAC7B,CAAC;MACD,MAAMK,GAAE,GAAI,IAAI,CAACC,SAAQ,GAAI,oBAAoB;MACjD7B,KAAI,CACD8B,IAAI,CACHF,GAAG,EACH;QAAEG,OAAO,EAAE,IAAI,CAACzB,MAAM;QAAE0B,aAAa,EAAE,IAAI,CAACtB;MAAa,CAAC,EAC1D;QAAEg