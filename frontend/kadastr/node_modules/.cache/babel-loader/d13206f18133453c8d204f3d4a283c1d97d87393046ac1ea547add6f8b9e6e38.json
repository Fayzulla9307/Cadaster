{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"DirectoryMenu\",\n  props: [],\n  watch: {},\n  data() {\n    return {\n      items: []\n    };\n  },\n  methods: {\n    loadMenu() {\n      const token = sessionStorage.getItem(\"token\");\n      const headers = {\n        \"Content-Type\": \"application/json\",\n        Authorization: \"Bearer \" + token\n      };\n      const url = this.$BASE_URL + \"/api/get_all_dictionaries/\";\n      axios.get(url, {\n        headers\n      }).then(response => {\n        if (!response.data.auth_fail) {\n          this.items = JSON.parse(JSON.stringify(response.data.result));\n          var count = 0;\n          for (var item of this.items) {\n            if (count == 0) {\n              item.selected = true;\n              this.loadDictionary(item.category, item.description);\n            } else {\n              item.selected = false;\n            }\n            count++;\n          }\n        }\n      });\n    },\n    loadDictionary(category) {\n      for (var item of this.items) {\n        if (item.category == category) {\n          item.selected = true;\n          this.$emit(\"item-clicked\", item);\n        } else {\n          item.selected = false;\n        }\n      }\n    }\n  },\n  components: {},\n  mounted() {\n    this.loadMenu();\n  }\n};","map":{"version":3,"names":["axios","name","props","watch","data","items","methods","loadMenu","token","sessionStorage","getItem","headers","Authorization","url","$BASE_URL","get","then","response","auth_fail","JSON","parse","stringify","result","count","item","selected","loadDictionary","category","description","$emit","components","mounted"],"sources":["/Users/fayzulla/Dev/kadastr/frontend/kadastr/src/components/DirectoryMenu.vue"],"sourcesContent":["<template>\n  <div class=\"menu\">\n    <div\n      v-for=\"(item, index) in items\"\n      class=\"menu-item\"\n      @click=\"loadDictionary(item.category)\"\n      v-bind:key=\"index\"\n      v-bind:class=\"item.selected ? 'selected-menu-item' : ''\"\n    >\n      {{ item.description }}\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"DirectoryMenu\",\n  props: [],\n  watch: {},\n  data() {\n    return {\n      items: [],\n    };\n  },\n  methods: {\n    loadMenu() {\n      const token = sessionStorage.getItem(\"token\");\n      const headers = {\n        \"Content-Type\": \"application/json\",\n        Authorization: \"Bearer \" + token,\n      };\n      const url = this.$BASE_URL + \"/api/get_all_dictionaries/\";\n      axios.get(url, { headers }).then((response) => {\n        if (!response.data.auth_fail) {\n          this.items = JSON.parse(JSON.stringify(response.data.result));\n          var count = 0;\n          for (var item of this.items) {\n            if (count == 0) {\n              item.selected = true;\n              this.loadDictionary(item.category, item.description);\n            } else {\n              item.selected = false;\n            }\n            count++;\n          }\n        }\n      });\n    },\n    loadDictionary(category) {\n      for (var item of this.items) {\n        if (item.category == category) {\n          item.selected = true;\n          this.$emit(\"item-clicked\", item);\n        } else {\n          item.selected = false;\n        }\n      }\n    },\n  },\n  components: {},\n  mounted() {\n    this.loadMenu();\n  },\n};\n</script>\n\n<style scoped>\n.menu {\n  min-width: 10%;\n  font-weight: bolder;\n  height: 100%;\n}\n\n.menu-item {\n  display: flex;\n  justify-content: left;\n  align-items: center;\n  border-radius: 5px;\n  min-width: calc(100% - 20px);\n  height: 55px;\n  color: #fff;\n  background-color: #372d69;\n  font-weight: bold;\n  margin-right: 20px;\n  margin-bottom: 8px;\n  padding-left: 20px;\n}\n\n.menu-item:hover {\n  background-color: #23185b;\n  cursor: pointer;\n}\n\n.selected-menu-item {\n  background-color: #23185b;\n  color: #fff9a4;\n}\n</style>\n"],"mappings":"AAeA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE,EAAE;EACTC,KAAK,EAAE,CAAC,CAAC;EACTC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MACT,MAAMC,KAAI,GAAIC,cAAc,CAACC,OAAO,CAAC,OAAO,CAAC;MAC7C,MAAMC,OAAM,GAAI;QACd,cAAc,EAAE,kBAAkB;QAClCC,aAAa,EAAE,SAAQ,GAAIJ;MAC7B,CAAC;MACD,MAAMK,GAAE,GAAI,IAAI,CAACC,SAAQ,GAAI,4BAA4B;MACzDd,KAAK,CAACe,GAAG,CAACF,GAAG,EAAE;QAAEF;MAAQ,CAAC,CAAC,CAACK,IAAI,CAAEC,QAAQ,IAAK;QAC7C,IAAI,CAACA,QAAQ,CAACb,IAAI,CAACc,SAAS,EAAE;UAC5B,IAAI,CAACb,KAAI,GAAIc,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,SAAS,CAACJ,QAAQ,CAACb,IAAI,CAACkB,MAAM,CAAC,CAAC;UAC7D,IAAIC,KAAI,GAAI,CAAC;UACb,KAAK,IAAIC,IAAG,IAAK,IAAI,CAACnB,KAAK,EAAE;YAC3B,IAAIkB,KAAI,IAAK,CAAC,EAAE;cACdC,IAAI,CAACC,QAAO,GAAI,IAAI;cACpB,IAAI,CAACC,cAAc,CAACF,IAAI,CAACG,QAAQ,EAAEH,IAAI,CAACI,WAAW,CAAC;YACtD,OAAO;cACLJ,IAAI,CAACC,QAAO,GAAI,KAAK;YACvB;YACAF,KAAK,EAAE;UACT;QACF;MACF,CAAC,CAAC;IACJ,CAAC;IACDG,cAAcA,CAACC,QAAQ,EAAE;MACvB,KAAK,IAAIH,IAAG,IAAK,IAAI,CAACnB,KAAK,EAAE;QAC3B,IAAImB,IAAI,CAACG,QAAO,IAAKA,QAAQ,EAAE;UAC7BH,IAAI,CAACC,QAAO,GAAI,IAAI;UACpB,IAAI,CAACI,KAAK,CAAC,cAAc,EAAEL,IAAI,CAAC;QAClC,OAAO;UACLA,IAAI,CAACC,QAAO,GAAI,KAAK;QACvB;MACF;IACF;EACF,CAAC;EACDK,UAAU,EAAE,CAAC,CAAC;EACdC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACxB,QAAQ,EAAE;EACjB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}